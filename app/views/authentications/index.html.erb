<h1>Listing authentications</h1>

<table>
  <tr>
    <th>User</th>
    <th>Provider</th>
    <th>Uid</th>
    <th>Email</th>
    <th>Image url</th>
    <th>Name</th>
    <th>Nickname</th>
    <th>Secret</th>
    <th>Token</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @authentications.each do |authentication| %>
  <tr>
    <td><%= authentication.user_id %></td>
    <td><%= authentication.provider %></td>
    <td><%= authentication.uid %></td>
    <td><%= authentication.email %></td>
    <td><%= authentication.image_url %></td>
    <td><%= authentication.name %></td>
    <td><%= authentication.nickname %></td>
    <td><%= authentication.secret %></td>
    <td><%= authentication.token %></td>
    <td><%= authentication.url %></td>
    <td><%= link_to 'Show', authentication %></td>
    <td><%= link_to 'Edit', edit_authentication_path(authentication) %></td>
    <td><%= link_to 'Destroy', authentication, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Authentication', new_authentication_path %>
