<h1>Listing user_settings</h1>

<table>
  <tr>
    <th>User</th>
    <th>Post sns with</th>
    <th>Post sns when</th>
    <th>Email me when</th>
    <th>Phone push me when</th>
    <th>Email agreed at</th>
    <th>Term agreed at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_settings.each do |user_setting| %>
  <tr>
    <td><%= user_setting.user_id %></td>
    <td><%= user_setting.post_sns_with %></td>
    <td><%= user_setting.post_sns_when %></td>
    <td><%= user_setting.email_me_when %></td>
    <td><%= user_setting.phone_push_me_when %></td>
    <td><%= user_setting.email_agreed_at %></td>
    <td><%= user_setting.term_agreed_at %></td>
    <td><%= link_to 'Show', user_setting %></td>
    <td><%= link_to 'Edit', edit_user_setting_path(user_setting) %></td>
    <td><%= link_to 'Destroy', user_setting, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User setting', new_user_setting_path %>
